{
  "name": "electron-ng-alain",
  "version": "0.0.0",
  "main": "./app/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --prod --build-optimizer",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:style",
    "e2e": "ng e2e",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "node scripts/color-less.js",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "hmr": "ng serve -c=hmr",
    "dev": "concurrently \"ng serve\" \"electron . --dev\"",
    "win-pack": "ng build --prod --build-optimizer && electron-builder -w"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@delon/abc": "^2.0.0-beta.5",
    "@delon/acl": "^2.0.0-beta.5",
    "@delon/auth": "^2.0.0-beta.5",
    "@delon/cache": "^2.0.0-beta.5",
    "@delon/chart": "^2.0.0-beta.5",
    "@delon/form": "^2.0.0-beta.5",
    "@delon/mock": "^2.0.0-beta.5",
    "@delon/theme": "^2.0.0-beta.5",
    "@delon/util": "^2.0.0-beta.5",
    "ajv": "^6.4.0",
    "core-js": "^2.5.4",
    "editorconfig-tools": "^0.1.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "ng-alain": "^2.0.0-beta.5",
    "ng-zorro-antd": "^1.6.0",
    "ngx-electron": "^1.0.4",
    "prettier": "^1.12.1",
    "prettier-stylelint": "^0.4.2",
    "rxjs": "~6.2.0",
    "screenfull": "^3.3.1",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-language-service": "^0.9.9",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.5",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "electron": "^3.0.5",
    "electron-builder": "^20.28.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less-bundle-promise": "^1.0.7",
    "ng-alain": "^2.0.0-beta.5",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.ts": [
      "npm run lint:ts",
      "prettier --write",
      "git add"
    ],
    "*.less": [
      "npm run lint:style",
      "prettier --write",
      "git add"
    ],
    "ignore": [
      "src/assets/*"
    ]
  },
  "build": {
    "appId": "con.Tast.app",
    "productName": "Tast",
    "directories": {
      "output": "win"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://localhost:5500/up/"
      }
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [
    "dist"
  ]
}
